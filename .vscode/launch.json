{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell",
            "type": "PowerShell",
            "request": "launch",
            "program": "${file}",
            "args": [Set-StrictMode -Version Latest, $debugpreference= "Continue"],
            "cwd": "${file}"
        },
        {
            "name": "PowerShell x86",
            "type": "PowerShell x86",
            "request": "launch",
            "program": "${file}",
            "args": [Set-StrictMode -Version Latest, $debugpreference= "Continue"],
            "cwd": "${file}"
        }
    ]
}